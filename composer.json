{
  "name": "mindedge/laravel-tenant",
  "description": "Simple trait to enable multi-tenant models",
  "keywords": [
    "mindedge",
    "laravel",
    "laravel-tenant"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Jacob Rogaishio",
      "email": "me@jacobrogaishio.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "goldspecdigital/laravel-eloquent-uuid": "^10.0",
    "illuminate/support": "^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "MindEdge\\LaravelTenant\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MindEdge\\LaravelTenant\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "MindEdge\\LaravelTenant\\LaravelTenantServiceProvider"
      ]
    }
  }
}
